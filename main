	if a:lower() == "e" then
		if e then
			e = false
		else
			e = true
			Fly()
		end
	elseif a:lower() == "w" then
		h.f = 1
	elseif a:lower() == "s" then
		h.b = -1
	elseif a:lower() == "a" then
