			end
		end
	else
	    local old = plr.Character:FindFirstChild("HumanoidRootPart").CFrame
    GetGun("M9")
    getChar():WaitForChild("HumanoidRootPart").CFrame = old
    wait(0.01)
    local Gun = ReturnGun()
		
		if Gun then
		for i = 1,55 do
			local Gen = {}
			for i =1,25 do
				Gen[#Gen+1]={
					["RayObject"] = Ray.new(Vector3.new(0,0,0), Vector3.new(0,0,0)),
